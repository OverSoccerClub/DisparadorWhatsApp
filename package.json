{
  "name": "whatsapp-dispatcher",
  "version": "0.1.10",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "cross-env FAST_REFRESH=false NODE_ENV=production next dev --turbo",
    "dev:safe": "npm run build && npm start",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "setup": "node scripts/setup.js",
    "update-supabase-env": "node scripts/update-supabase-env.js",
    "check-supabase": "node scripts/check-supabase-config.js",
    "setup-waha": "powershell -ExecutionPolicy Bypass -File scripts/setup-waha.ps1",
    "setup-waha-db": "node scripts/setup-waha.js",
    "fix-waha": "powershell -ExecutionPolicy Bypass -File scripts/fix-waha-complete.ps1",
    "test-waha": "node test-waha-setup.js",
    "clear-cache": "powershell -ExecutionPolicy Bypass -File scripts/clear-cache.ps1",
    "clear-cache-unix": "bash scripts/clear-cache.sh",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy-unix": "bash scripts/deploy.sh",
    "build:prod": "cross-env NODE_ENV=production next build",
    "start:prod": "cross-env NODE_ENV=production node .next/standalone/server.js",
    "docker:build": "docker build -t whatsapp-dispatcher .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod-stop": "docker-compose -f docker-compose.prod.yml down",
    "version:patch": "node scripts/version.js patch",
    "version:minor": "node scripts/version.js minor",
    "version:major": "node scripts/version.js major",
    "version:set": "node scripts/version.js set",
    "version:show": "node scripts/version.js show",
    "smoke": "node scripts/smoke-check.js",
    "worker": "ts-node -r tsconfig-paths/register scripts/worker.ts",
    "build:worker": "tsup scripts/worker.ts --format cjs --out-dir dist/scripts --platform node --silent",
    "worker:prod": "node dist/scripts/worker.js",
    "version:push": "git push origin main && git push --tags",
    "version:init": "git init && git add . && git commit -m \"Initial commit\" && node scripts/version.js set 0.1.0",
    "git:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-git.ps1"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.38.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "baileys": "^6.6.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "clsx": "^2.0.0",
    "critters": "^0.0.23",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "^14.2.15",
    "papaparse": "^5.4.1",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.15.2",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.14",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5"
  }
}
